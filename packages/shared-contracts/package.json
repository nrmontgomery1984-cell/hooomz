      "default": "./src/schemas/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    },
    "./api": {
      "types": "./src/api/index.ts",
      "default": "./src/api/index.ts"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "verify": "node verify-exports.js"
  },
  "dependencies": {
    "zod": "^3.22.4",
